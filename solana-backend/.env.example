# Configurazione Solana
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_NETWORK=mainnet-beta

# Configurazione API
PORT=3001
NODE_ENV=production

# Configurazione CORS
CORS_ORIGIN=*

# Configurazione Helius API
HELIUS_API_KEY=your_helius_api_key_here
HELIUS_RPC_URL=https://mainnet.helius-rpc.com

# Configurazione Rate Limiting
API_RATE_LIMIT_DELAY=100
API_REQUEST_TIMEOUT=10000

# Configurazione Cache
CACHE_TTL=300000
CACHE_ENABLED=true

# Configurazione GitHub (opzionale)
GITHUB_TOKEN=
GITHUB_REPO_NAME=solana-token-dashboard
GITHUB_USERNAME=

# Configurazione Deploy (opzionale)
VERCEL_TOKEN=
RENDER_API_KEY=